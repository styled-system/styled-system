{
  "name": "sketch",
  "private": true,
  "version": "1.0.0",
  "description": "Example using styled-system and react-sketchapp",
  "skpm": {
    "main": "styled-system.sketchplugin",
    "manifest": "manifest.json"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "render": "skpm-build --watch --run",
    "render:once": "skpim-build --run",
    "prepare": "npm run build && skpm-link"
  },
  "license": "MIT",
  "dependencies": {
    "@skpm/builder": "^0.4.1",
    "react": "^16.2.0",
    "react-native": "^0.54.0",
    "react-primitives": "^0.5.0",
    "react-sketchapp": "^1.0.0",
    "react-test-renderer": "^16.2.0",
    "styled-components": "^2.1.0",
    "styled-system": "^2.1.1"
  }
}

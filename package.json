{
  "name": "@tradersclub/styled-system",
  "typings": "index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "prettier": "prettier \"src/**/{src,test,examples}/**/*.js\" --write",
    "prettier:docs": "prettier \"docs/**/*.{js,md}\" --write",
    "clean": "rm -rf dist",
    "build": "npx rollup -c",
    "logo": "npx repng docs/src/logo/index.js -d docs/static -f logo.png -w 512 -h 512",
    "logo-svg": "npx scrs docs/src/logo/index.js --svg > docs/static/logo.svg",
    "logo-white": "npx scrs docs/src/logo/white.js --svg > docs/static/logo-white.svg",
    "benchmark": "yarn workspace benchmarks test",
    "cover": "yarn test --coverage",
    "codecov": "yarn cover && npx codecov",
    "start": "yarn workspace @styled-system/docs start",
    "docs": "yarn workspace @styled-system/docs start",
    "docs:build": "yarn workspace @styled-system/docs build",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/plugin-transform-runtime": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/register": "7.13.16",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@types/styled-components": "5.1.12",
    "csstype": "3.0.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-emotion": "10.0.32",
    "jest-styled-components": "7.0.4",
    "lerna": "3.22.1",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "react": "16.13.1",
    "react-test-renderer": "16.13.1",
    "styled-components": "5.1.1"
  },
  "peerDependencies": {
    "styled-components": ">=5"
  },
  "jest": {
    "testMatch": [
      "**/src/**/test/*.js"
    ],
    "collectCoverageFrom": [
      "**/src/**/src/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tradersclub/styled-system.git"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^10.0.14",
    "color": "^3.1.0",
    "eslint-plugin-react": "7.24.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2",
    "react-scripts": "^3.0.1"
  },
  "version": "5.7.0"
}
